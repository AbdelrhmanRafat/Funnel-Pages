---
import type { BlockData, Product } from "../../../../../lib/api/types";

const data: BlockData[] = Astro.props.data;
const product : Product = Astro.props.product;

import { FunnelArabicTouchComponents } from "../../../../../lib/constants/themes";
import ArabicTouchProductCosts from "../UI/ArabicTouchProductCosts/arabictouchProductCosts.astro";
import ArabicTouchBundleOptions from "../UI/ArabicTouchBundleOptions/ArabicTouchBundleOptions.astro";

---

<div id={FunnelArabicTouchComponents.ArabicTouchProductFunnel} class="w-full flex flex-col gap-2">
  <ArabicTouchBundleOptions data={data} product={product} />
  <ArabicTouchProductCosts hasBundles="true" />
</div>