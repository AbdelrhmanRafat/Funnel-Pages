---
import type { BlockData, Product } from "../../../../../lib/api/types";

const data: BlockData[] = Astro.props.data;
const product : Product = Astro.props.product;

import { FunnelNasaComponents } from "../../../../../lib/constants/themes";
import NasaProductCosts from "../UI/NasaProductCosts/nasaProductCosts.astro";
import NasaBundleOptions from "../UI/NasaBundleOptions/NasaBundleOptions.astro";

---

<div id={FunnelNasaComponents.NasaProductFunnel} class="w-full flex flex-col gap-2">
  <NasaBundleOptions data={data} product={product} />
  <NasaProductCosts hasBundles="true" />
</div>